{
  "name": "vue_posts",
  "version": "1.0.0",
  "description": "A vuejs application",
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv/config app.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "node -r dotenv/config app.js",
    "build": "vite build && npm run purgecss",
    "serve": "vite preview",
    "purgecss": "purgecss --config ./purgecss.config.js",
    "test": "SET TEST=1 && mocha --timeout 10000 --exit"
  },
  "author": "DaveDeDave",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.28.2",
    "better-sqlite3": "^7.5.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dompurify": "^2.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsdom": "^18.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@tiptap/extension-highlight": "^2.0.0-beta.26",
    "@tiptap/starter-kit": "^2.0.0-beta.133",
    "@tiptap/vue-3": "^2.0.0-beta.75",
    "@vitejs/plugin-vue": "^1.9.3",
    "axios": "^0.24.0",
    "concurrently": "^6.3.0",
    "mocha": "^9.2.0",
    "purgecss": "^4.0.3",
    "sass": "^1.43.3",
    "supertest": "^6.2.2",
    "swiper": "^7.0.0",
    "vite": "^2.6.4",
    "vue": "^3.2.16",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.11"
  }
}
